            "& when (@bg-type = \"image\")": {
              "background-image": "url(@bg-value)",
              ".mbr-overlay": {
                "background-color": "@overlayColor",
                "opacity": "@overlayOpacity"
              }
            },
            ".title-wrap, .title": {
              "width": "100%",
              "padding": "0 1rem"
            },
            ".list-item": {
              "padding": "1rem 0"
            },
            ".mbr-section-btn": {
              "margin": "0"
            },
            ".card-head": {
              "padding-top": "0",
              "padding-bottom": "0rem"
            },
            ".card": {
              "&:hover": {
                ".btn": {
                  "background-color": "@primaryColor",
                  "color": "#ffffff !important",
